{
  "manifestVersion": 1.0,
  "id": "hub-tfs",
  "version": "1.0.0",
  "name": "Black Duck Hub",
  "publisher": "black-duck-software",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": [
    "Black Duck",
    "Continuous Integration",
    "Security",
    "Open Source",
    "Hub"
  ],
  "description": "Execute a Black Duck Hub scan against your build",
  "categories": [
    "Integrate"
  ],
  "icons": {
    "default": "images/bds_ducky.png"
  },
  "branding": {
    "color": "#00639c",
    "theme": "dark"
  },
  "files": [
    {
      "path": "Tasks/tfs-scan-executor"
    },
    {
      "path": "images",
      "addressable": true
    }
  ],
  "content": {
    "details": {
      "path": "overview.md"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/blackducksoftware/hub-tfs"
    }
  },
  "contributions": [
    {
      "id": "hub.tfs.scan",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "Tasks/tfs-scan-executor"
      }
    }
  ]
}